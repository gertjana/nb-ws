GET         /user                       UserApi.listUsers
GET         /user/{id}                  UserApi.getUser
PUT         /user                       UserApi.newUser
POST        /user/{id}                  UserApi.updateUser
POST        /user/{email}/{name}        UserApi.registerUser
DELETE      /user/{id}                  UserApi.deleteUser

GET         /static*                    Default.handle
*           *                           Default.refuse

